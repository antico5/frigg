$('#star').raty({
  readOnly: true,
  score: <%= @post.average_rating %>,
});

$('#user_star').raty({
  score: <%= @rating.score %>,
  click: function(score, evt) {
    $.ajax({
      url: '/ratings/' + <%= @rating.id %>,
      type: 'PATCH',
      data: { score: score }
    }).done(function (data){});
  }
});
